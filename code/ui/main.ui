QPushButton {
  border-radius: 12px;
  padding: 8px 12px;
  /* Default icon size if not set in Designer */
  icon-size: 24px 24px;
}

/* ----------------------------------------------------
   ICON COLOR FIX: Must load a different image file per state/color
   ---------------------------------------------------- */

QPushButton[class=&quot;active&quot;] {
  background: white;
  color: #1e40af;
  font-weight: bold;
  /* Assuming 'active' requires a different colored icon, e.g., blue */
}

QPushButton[class=&quot;primary&quot;] {
  background: rgb(255, 255, 255);
  color: #1e40af; 
  font-weight: bold;
  /* Assuming 'primary' requires a different colored icon, e.g., yellow */
}

QPushButton[class=&quot;danger&quot;] {
  background: transparent;
  color: white;
  font-weight: bold;
  border: 2px solid rgb(248, 68, 68);
  /* The default white icon should show well against a transparent background */
}
QPushButton[class=&quot;danger&quot;]:hover {
  background: rgb(248, 68, 68);
  border: 2px solid rgb(248, 68, 68);
}

QPushButton[class=&quot;deactive&quot;] {
  background: transparent;
  color: white;
  border: 1px solid rgba(255, 255, 255, 0.18);
}

QPushButton[class=&quot;deactive&quot;]:hover {
  color: white;
  border: 1px solid rgba(255, 255, 255, 0.2);
  background-color: rgba(255, 255, 255, 0.2);
}

QPushButton[class=&quot;ghost&quot;]:hover {
  border: 1px solid rgba(255, 255, 255, 0.2);
  background-color: rgba(255, 255, 255, 0.2);
  /* Example: Change icon to a light gray version on hover */
}

QPushButton[class=&quot;ghost&quot;] {
  color: white;
  background-color: transparent;
  border: 1px solid rgba(255, 255, 255, 0.2);
  /* Load the default white icon */
}

QLineEdit,
QComboBox,
QSpinBox {
  background: rgba(255, 255, 255, 0.06);
  border: 1px solid rgba(255, 255, 255, 0.12);
  border-radius: 10px;
